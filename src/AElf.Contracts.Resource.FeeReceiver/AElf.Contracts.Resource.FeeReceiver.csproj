<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="$(SolutionDir)\AElf.Contract.Tools.targets"/>
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>AElf.Contracts.Resource.FeeReceiver</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\AElf.Sdk.CSharp\AElf.Sdk.CSharp.csproj"/>
    </ItemGroup>

    <ItemGroup>
        <ContractReference Include="$(SolutionDir)\protobuf\token_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\token_contract.proto</Link>
        </ContractReference>
    </ItemGroup>

    <ItemGroup>
        <ContractCode Include="$(SolutionDir)\protobuf\fee_receiver_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\fee_receiver_contract.proto</Link>
        </ContractCode>
    </ItemGroup>

</Project>
