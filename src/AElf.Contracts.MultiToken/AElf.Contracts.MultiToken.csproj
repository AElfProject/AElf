<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>AElf.Contracts.MultiToken</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\AElf.Sdk.CSharp\AElf.Sdk.CSharp.csproj" />
    </ItemGroup>

    <Import Project="$(SolutionDir)\AElf.Contract.Tools.targets" />

    <ItemGroup>
        <ContractReference Include="$(SolutionDir)\protobuf\acs0.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\acs0.proto</Link>
        </ContractReference>
        <ContractReference Include="$(SolutionDir)\protobuf\cross_chain_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\cross_chain_contract.proto</Link>
        </ContractReference>
    </ItemGroup>
    <ItemGroup>
        <ContractBase Include="$(SolutionDir)\protobuf\acs1.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\acs1.proto</Link>
        </ContractBase>
        <ContractBase Include="$(SolutionDir)\protobuf\acs2.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\acs2.proto</Link>
        </ContractBase>
        <ContractBase Include="$(SolutionDir)\protobuf\token_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\token_contract.proto</Link>
        </ContractBase>
        <ContractCode Include="$(SolutionDir)\protobuf\token_contract_impl.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\token_contract_impl.proto</Link>
        </ContractCode>
    </ItemGroup>
</Project>
