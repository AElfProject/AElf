    "ClusterDbNumber": 0,
    "GrainStorageDbConnection": "127.0.0.1:6379",
    "GrainStorageDbNumber": 0
  },
  
  "ConnectionStrings": {
    "Default": "mongodb://127.0.0.1:27017/AElf.Kernel.SmartContract"
  },
  
  "Contract": {
    "Delay": 3000,
    "RetryDelay": 2000,
    "RetryTimes": 12,
    "SafeBlockHeight": 50
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
 
  "Serilog": {
    "MinimumLevel": {
