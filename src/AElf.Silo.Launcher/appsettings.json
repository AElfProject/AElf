{
  "ChainId": "AELF",
  "ChainType": "MainChain",
  "NetType": "TestNet",
  "ConnectionStrings": {
    "BlockchainDb": "redis://localhost:6379?db=1",
    "StateDb": "redis://localhost:6379?db=1"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5001/"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "LaunchProgram":"Silo",
  "Orleans": {
    "ClusterId": "AELFSiloServerSiloClusterBP1",
    "ServiceId": "AELFSiloServerOrleansBasicServiceBP1",
    "AdvertisedIP": "127.0.0.1",
    "GatewayPort": 20002,
    "SiloPort": 10002,
    "MongoDBClient": "mongodb://127.0.0.1:27017/?maxPoolSize=555",
    "DataBase": "AELFSiloServerOrleansDB1",
    "DashboardUserName": "admin",
    "DashboardPassword": "123456",
    "DashboardCounterUpdateIntervalMs": 1000,
    "DashboardPort": 8009,
    "EventStoreConnection": "ConnectTo=tcp://localhost:1113; HeartBeatTimeout=500",
    "ClusterDbConnection": "127.0.0.1:6379",
    "ClusterDbNumber": 0,
    "GrainStorageDbConnection": "127.0.0.1:6379",
    "GrainStorageDbNumber": 0
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": "Logs/log-.log",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15
        }
      }
    ]
  }
}