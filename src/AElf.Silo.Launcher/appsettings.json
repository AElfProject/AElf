{
  "Orleans": {
    "ClusterId": "AELFSiloServerSiloCluster",
    "ServiceId": "AELFSiloServerOrleansBasicService",
    "AdvertisedIP": "127.0.0.1",
    "GatewayPort": 20001,
    "SiloPort": 10001,
    "MongoDBClient": "mongodb://127.0.0.1:27017/?maxPoolSize=555",
    "DataBase": "AELFSiloServerOrleansDB",
