<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <RootNamespace>TokenConverter</RootNamespace>
        <PackageId>AElf.Contracts.TokenConverter</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\AElf.Sdk.CSharp\AElf.Sdk.CSharp.csproj" />
    </ItemGroup>

    <Import Project="$(SolutionDir)\AElf.Contract.Tools.targets" />

    <ItemGroup>
        <ContractReference Include="$(SolutionDir)\protobuf\token_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\reference\token_contract.proto</Link>
        </ContractReference>
    </ItemGroup>
    <ItemGroup>
        <ContractCode Include="$(SolutionDir)\protobuf\token_converter_contract.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\token_converter_contract.proto</Link>
        </ContractCode>
    </ItemGroup>
</Project>