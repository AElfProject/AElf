<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>AElf.Kernel.SmartContract.ExecutionPluginForAcs5</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <NoWarn>1998</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\AElf.Kernel.SmartContract\AElf.Kernel.SmartContract.csproj" />
    </ItemGroup>

    <ItemGroup>
        <ContractStub Include="..\..\protobuf\acs5.proto">
            <Link>Protobuf/Proto/acs5.proto</Link>
        </ContractStub>
        <ContractStub Include="..\..\protobuf\token_contract.proto">
            <Link>Protobuf/Proto/token_contract.proto</Link>
        </ContractStub>
    </ItemGroup>
</Project>
