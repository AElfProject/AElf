        </ContractCode>
        <ContractInternal Include="$(ProtoBaseDir)\dpos_sidechain_contract_internal.proto">
            <Link>$(ProjectDir)\Protobuf\Proto\internal\dpos_sidechain_contract_internal.proto</Link>
        </ContractInternal>
    </ItemGroup>
</Project>
