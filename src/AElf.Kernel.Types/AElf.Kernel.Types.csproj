    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\AElf.Types\AElf.Types.csproj" />
    </ItemGroup>
    <ItemGroup>
        <Compile Remove="IResourcePath.cs" />
    </ItemGroup>
    <ItemGroup>
        <CommonMessage Include="..\..\protobuf\kernel.proto">
